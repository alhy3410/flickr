<%= form_for @photo, :html => { multipart: true  } do |f| %>
<%= f.label :photographer, "Photographer" %>
<%= f.text_field :photographer %>
<br>
<%= f.label :datetaken, "Date Taken" %>
<%= f.text_field :datetaken %>
<br>
<%= f.label :camera, "Camera Used" %>
<%= f.text_field :camera %>
<br>
<%= f.label :details, "Details About The Photo" %>
<br>
<%= f.text_area :details, size: "100x10" %>
<br>
<%= f.label :image %>
<%= f.file_field :image %>
<br>
<%= f.submit %>
<% end %>
